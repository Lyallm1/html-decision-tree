{
  "COMPILEONSAVE": true,
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "declaration": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "downlevelIteration": true,
    "noLib": false,
    "alwaysStrict": false,
    "sourceMap": true,
    "lib": ["es6", "dom"],
    "outDir": "./dist/"
  }, "include": ["ts/**/*"],
  "exclude": ["dist", "samples", "node_modules"]
}
